folder_path_counts <- "dat/counts"
folder_path_DEGs <- "dat/DEGs"
# Function to compress data files to .RDS
compress_to_rds <- function(folder_path) {
# Get a list of all files in the folder
file_list <- list.files(path = folder_path, pattern = "\\.txt$", full.names = TRUE)
# Loop through the list of files and save them as .RDS
for (file_path in file_list) {
data <- read.table(file_path, header = TRUE, sep = "\t")  # Adjust the reading function and separator
rds_file_path <- sub("\\.txt$", ".rds", file_path)  # Create a new .RDS file name
saveRDS(data, file = rds_file_path)  # Save as .RDS
}
}
# Compress data from 'dat/counts' folder
compress_to_rds(folder_path_counts)
# Compress data from 'dat/DEGs' folder
compress_to_rds(folder_path_DEGs)
